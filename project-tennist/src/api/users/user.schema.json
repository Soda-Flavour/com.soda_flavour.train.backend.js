{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "User",
  "description": "유저의 스키마",
  "default": {},
  "examples": [
    {
      "id": 1,
      "email": "axa8380@gmail",
      "nick": "소다맛 환타",
